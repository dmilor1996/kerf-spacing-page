            var _aSpeakList,_speakListTimer=null,_utterList,_speakListIndex=0,_btnSpeakList,_speechDelay=5E3,_isSpeakList=!1,_isMac=/iPad|iPhone|iPod|Mac/i.test(navigator.userAgent),_canRecognise=("webkitSpeechRecognition"in window||"SpeechRecognition"in window)&&!_isMac,_canSleep=!0;let wakeLock=null;
            function _ToggleSpeakList(a,c){IsMobile()&&_canSleep&&((async()=>{try{wakeLock=await navigator.wakeLock.request("screen"),wakeLock.addEventListener("release",b=>{_canSleep=!0})}catch(b){}})(),_canSleep=!1);(_isSpeakList=!_isSpeakList)?(_aSpeakList=$(c).innerHTML.replace(/<br>/g,",").replace(/(<([^>]+)>|\s|&nbsp;)/ig,"").replace(/[\s]?to[\s]?/g,",").replace(/-/g,"- ").replace(/_/g," ").split(","),_utterList||=new SpeechSynthesisUtterance,(c=prompt("Speech Interval in Seconds",3))&&!isNaN(c)?(_speechDelay=
            parseInt(1E3*c),_speakListIndex=0,_btnSpeakList=a,_utterList.onend=()=>{_speakListTimer=setTimeout("_SpeakListLoop()",_speechDelay)},_utterList.text="Ready",speechSynthesis.speak(_utterList),a.value="Speech Off",a.className="BtnSmallRed"):_isSpeakList=!1):(_utterList.onend=null,clearTimeout(_speakListTimer),_btnSpeakList.value="Speak",_btnSpeakList.className="BtnSmall",_isSpeakList=!1,IsMobile&&wakeLock&&(wakeLock.release(),wakeLock=null),a.value="Speak",a.className="BtnSmall")}
            function _SpeakListLoop(){if(_isSpeakList&&_speakListIndex<_aSpeakList.length){var a=_aSpeakList[_speakListIndex],c="Mark.!.. ";if(isNaN(a))c+=GetSpokenImp(a);else{if(3<a.length){a=CommaFormat(a);var b=a.split(",");3>b.length&&(a="1"===b[0]?b[0]+" Meter ":b[0]+" Meters ",b=parseInt(b[1]),0<b&&(100>b&&(a+=" and "),a+=b))}c+=a}_speakListIndex===_aSpeakList.length-1&&(c="Last, "+c,_utterList.onend=()=>{_utterList.onend=null;clearTimeout(_speakListTimer);_btnSpeakList.value="Speak";_btnSpeakList.className=
            "BtnSmall";_isSpeakList=!1;IsMobile&&wakeLock&&(wakeLock.release(),wakeLock=null)});_utterList.text=c;speechSynthesis.speak(_utterList);_speakListIndex++}};
